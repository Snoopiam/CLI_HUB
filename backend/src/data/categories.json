  "taskPatterns": [
    {
      "pattern": "create|build|make|develop|implement",
      "boost": ["skills", "agents"],
      "description": "Creation tasks benefit from specialized skills and agents"
    },
    {
      "pattern": "fix|debug|solve|troubleshoot|error",
      "boost": ["agents"],
      "priorityAgents": ["debugger"],
      "description": "Debugging tasks should prioritize debugger agent"
    },
    {
      "pattern": "review|audit|check|analyze",
      "boost": ["agents", "hooks"],
      "priorityAgents": ["code-reviewer", "security-auditor"],
      "description": "Review tasks benefit from reviewer agents"
    },
    {
      "pattern": "deploy|release|ship|publish",
      "boost": ["mcps", "hooks"],
      "priorityAgents": ["deployment-engineer"],
      "description": "Deployment tasks need MCP integrations"
    },
    {
      "pattern": "optimize|improve|speed|performance",
      "boost": ["agents"],
      "priorityAgents": ["performance-engineer"],
      "description": "Optimization tasks need performance expertise"
    },
    {
      "pattern": "test|coverage|tdd|qa",
      "boost": ["agents", "commands"],
      "priorityAgents": ["test-automator"],
      "description": "Testing tasks need test automation"
    },
    {
      "pattern": "document|readme|api-doc",
      "boost": ["claudemd", "commands"],
      "description": "Documentation tasks need CLAUDE.md and doc commands"
    },
    {
      "pattern": "secure|protect|vulnerability|compliance",
      "boost": ["agents", "settings", "hooks"],
      "priorityAgents": ["security-auditor"],
      "description": "Security tasks need comprehensive protection"
    },
    {
      "pattern": "integrate|connect|api|external",
      "boost": ["mcps", "skills"],
      "description": "Integration tasks benefit from MCP servers"
    },
    {
      "pattern": "setup|configure|init|start",
      "boost": ["claudemd", "settings"],
      "description": "Setup tasks need project configuration"
    }
  ],
  "complexityIndicators": {
